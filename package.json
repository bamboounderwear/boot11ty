{
  "name": "eleventy-bootstrap-portfolio",
  "version": "1.0.0",
  "description": "Portfolio website built with 11ty and Bootstrap 5",
  "scripts": {
    "start": "npm-run-all clean -p watch:*",
    "dev": "npm run start",
    "build": "npm-run-all clean -p build:*",
    "watch:sass": "sass --watch src/scss:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass src/scss:_site/css",
    "build:eleventy": "eleventy",
    "clean": "rimraf _site"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "bootstrap": "^5.3.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1"
  }
}